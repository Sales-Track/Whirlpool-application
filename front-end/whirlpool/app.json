{
  "expo": {
    "name": "whirlpool",
    "slug": "whirlpool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/WHIRLPOOL_LOGO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/WHIRLPOOL_LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "permissions": ["INTERNET"]
    },
    "plugins": [
      [
          "expo-build-properties",
          {
              "android": {
                  "usesCleartextTraffic": true
              }
          }
      ]
  ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/WHIRLPOOL_LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.salah123.whirlpool"
    },
    "web": {
      "favicon": "./assets/WHIRLPOOL_LOGO.png"
    },
    "extra": {
      "eas": {
        "projectId": "f92ca845-1d3e-4218-8d4b-44e66fef7bf5"
      }
    }
  }
}
